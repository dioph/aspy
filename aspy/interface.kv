FloatLayout:

<GridLayout>:
    padding: 10

<BoxLayout>:
    padding: 10

<Interface>:
    GridLayout:
        cols: 3
        on_parent: root.variables()

        GridLayout:
            rows: 5
            id: elements

            ToggleButton:
                id: trafo
                text: "!"
                group: "elements"
                on_press: root.get_pressed_toggle_btn_symbol(elements)

            ToggleButton:
                id: pv
                text: "@"
                group: "elements"
                on_press: root.get_pressed_toggle_btn_symbol(elements)

            ToggleButton:
                id: pq
                text: "#"
                group: "elements"
                on_press: root.get_pressed_toggle_btn_symbol(elements)

            ToggleButton:
                id: lt
                text: "$"
                group: "elements"
                on_press: root.get_pressed_toggle_btn_symbol(elements)

            ToggleButton:
                id: s
                text: "%"
                group: "elements"
                on_press: root.get_pressed_toggle_btn_symbol(elements)

        BoxLayout:
            GridLayout:
                id: grid
                cols: 10
                rows: 10
                on_parent: root.add_grid(self)

